{
    "manifest_version": 3,
    "name": "Toggle Audio Tabs",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_icon": "./icons/icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "./icons/icon16.png",
        "48": "./icons/icon48.png",
        "128": "./icons/icon128.png"
    }
}